datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AuditLog {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model CrudLog {
  action    String?
  createdAt DateTime  @default(now())
  date      DateTime?
  entite    String?
  id        String    @id @default(cuid())
  updatedAt DateTime  @updatedAt
}
